{
    "name": "jakubgucen/entity-constants-generator",
    "type": "library",
    "description": "Generates constants containing field names and adds methods in each entity: get, set, add, remove.",
    "keywords": [
        "php",
        "entity",
        "doctrine",
        "orm",
        "constant",
        "constants",
        "get",
        "set",
        "add",
        "remove"
    ],
    "license": "MIT",
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "Jakub Gucenowicz",
            "email": "jakubgucen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-mbstring": "*"
    }
}
